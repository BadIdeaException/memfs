{
  "name": "memfs",
  "version": "2.0.0",
  "description": "In-memory file-system with Node's fs API.",
  "main": "build/index.js",
  "keywords": [
    "fs", "fs.js", "memory-fs", "memfs",
    "file", "file system", "mount", "memory",
    "in-memory", "virtual", "test", "testing", "mock"],
  "repository": {
    "type" : "git",
    "url" : "https://github.com/streamich/memfs.git"
  },
  "scripts": {
    "build":        "npm run build-ts && npm run build-js",
    "build-ts":     "./node_modules/.bin/gulp build-ts",
    "build-js":     "./node_modules/.bin/babel src --out-dir lib",
    "test":         "./node_modules/.bin/mocha --require ts-node/register ./src/**/**/**/**/**/**/*.test.ts",
    "test-watch":   "./node_modules/.bin/mocha --require ts-node/register ./src/**/**/**/**/**/**/*.test.ts --watch"
  },
  "dependencies": {
    "fast-extend": "0.0.2"
  },
  "devDependencies": {
    "mocha": "3.4.2",
    "chai": "4.1.0",
    "typescript": "2.4.2",
    "ts-node": "3.3.0",
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "gulp": "3.9.1",
    "gulp-typescript": "3.2.1",

    "@types/mocha": "2.2.41",
    "@types/chai": "4.0.1",
    "@types/node": "8.0.17"
  }
}
